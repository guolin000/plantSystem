<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.plantsys.mapper.CountyMapper">

    <resultMap id="BaseResultMap" type="com.plantsys.entity.County">
            <id property="countyId" column="county_id" jdbcType="INTEGER"/>
            <result property="countyName" column="county_name" jdbcType="VARCHAR"/>
            <result property="cityId" column="city_id" jdbcType="INTEGER"/>
