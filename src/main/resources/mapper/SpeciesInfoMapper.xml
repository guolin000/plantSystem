        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.plantsys.mapper.SpeciesInfoMapper">

    <resultMap id="BaseResultMap" type="com.plantsys.entity.SpeciesInfo">
            <result property="speciesId" column="species_id" jdbcType="INTEGER"/>
            <result property="speciesName" column="species_name" jdbcType="VARCHAR"/>
            <result property="genusName" column="genus_name" jdbcType="VARCHAR"/>
            <result property="familyName" column="family_name" jdbcType="VARCHAR"/>
